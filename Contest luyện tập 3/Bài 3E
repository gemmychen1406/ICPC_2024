#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    int n;
    cin >> n;
    vector<int> arr(n);
    
    for (int i = 0; i < n; ++i) {
        cin >> arr[i];
    }
    
    vector<int> left_max(n), right_min(n);
    
    left_max[0] = arr[0];
    for (int i = 1; i < n; ++i) {
        left_max[i] = max(left_max[i - 1], arr[i]);
    }
    
    right_min[n - 1] = arr[n - 1];
    for (int i = n - 2; i >= 0; --i) {
        right_min[i] = min(right_min[i + 1], arr[i]);
    }
    
    vector<int> pivots;
    for (int i = 0; i < n; ++i) {
        if (((left_max[i - 1] <= arr[i]) || (i == 0)) && ((right_min[i + 1] >= arr[i]) || (i == (n - 1)))) pivots.push_back(arr[i]);
    }
    
    int m = pivots.size();
    cout << m << " ";
    for (int i = 0; i < min(m, 100); ++i) {
        cout << pivots[i] << " ";
    }
    
    return 0;
}
